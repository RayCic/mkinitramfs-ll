if [ -n "$(echo "$izfs" | cut -d',' -f3)" ]; then
	debug -d source "$moddir"/zfs
	debug -d dozfs "$(echo "$iroot" | cut -d':' -f1)" "$(echo "$izfs" | cut -d',' -f1)"
	export swap=resume
fi
