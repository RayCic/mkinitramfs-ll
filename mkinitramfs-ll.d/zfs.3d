# $Id: mkinitramfs-ll.d/zfs.3d,v 0.12.0 2012/11/09 20:30:37 -tclover Exp $

set +x
source $moddir/zfs || exit

if [ -n "$(echo "$izfs" | cut -d, -f1)" ] || [ "${iroot##*:}" = "zfs" ]; then
	debug -d source "$moddir"/zfs
	debug -d dozfs "$(echo "$iroot" | cut -d: -f1)" "$(echo "$izfs" | cut -d, -f1)" -m
fi

echo 'root=mounted' >> /run/env

